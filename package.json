{
  "name": "literalify",
  "author": "Alan Plum <me@pluma.io>",
  "version": "0.4.0",
  "license": "MIT",
  "description": "A browserify transform for replacing require calls with arbitrary code.",
  "keywords": [
    "browserify",
    "transform",
    "modules",
    "browser",
    "globals",
    "commonjs",
    "require"
  ],
  "main": "./index.js",
  "files": [
    "README.md",
    "LICENSE",
    "index.js"
  ],
  "dependencies": {
    "browserify-transform-tools": "^1.3.0"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "coveralls": "^2.11.2",
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.6",
    "jshint": "^2.6.3",
    "mocha": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pluma/literalify.git"
  },
  "scripts": {
    "lint": "jshint index.js test",
    "test": "mocha --growl -R spec",
    "cover": "istanbul cover --report lcov _mocha -- -R spec",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls ; rm -rf ./coverage"
  },
  "engines": {
    "node": ">=0.6"
  }
}